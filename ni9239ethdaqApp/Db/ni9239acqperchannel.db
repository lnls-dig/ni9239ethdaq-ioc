record(ai, "$(P)$(R)Current$(CH)-Mon") {
  field(DESC, "Channel Current $(CH)")
  field(PINI, "YES")
  field(DTYP, "stream")
  field(SCAN, "1 second")
  field(EGU, "A")
  field(INP, "@ni9239acq.proto getMeasure($(CH)) $(PORT)")
}

